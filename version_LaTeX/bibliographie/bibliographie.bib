@misc{souza_using_2016,
	title = {Using Artificial Intelligence to Identify State Secrets},
	url = {http://arxiv.org/abs/1611.00356},
	doi = {10.48550/arXiv.1611.00356},
	abstract = {Whether officials can be trusted to protect national security information has become a matter of great public controversy, reigniting a long-standing debate about the scope and nature of official secrecy. The declassification of millions of electronic records has made it possible to analyze these issues with greater rigor and precision. Using machine-learning methods, we examined nearly a million State Department cables from the 1970s to identify features of records that are more likely to be classified, such as international negotiations, military operations, and high-level communications. Even with incomplete data, algorithms can use such features to identify 90\% of classified cables with {\textless}11\% false positives. But our results also show that there are longstanding problems in the identification of sensitive information. Error analysis reveals many examples of both overclassification and underclassification. This indicates both the need for research on inter-coder reliability among officials as to what constitutes classified material and the opportunity to develop recommender systems to better manage both classification and declassification.},
	number = {{arXiv}:1611.00356},
	publisher = {{arXiv}},
	author = {Souza, Renato Rocha and Coelho, Flavio Codeco and Shah, Rohan and Connelly, Matthew},
	urldate = {2024-07-10},
	date = {2016-11-01},
